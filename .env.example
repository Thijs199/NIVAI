# AIFAA Environment Configuration Example
# Copy this file to .env and fill in your actual values

# External Data Storage Configuration
AZURE_STORAGE_ACCOUNT_NAME=nivaistorage
AZURE_STORAGE_ACCOUNT_KEY=your-storage-account-key
AZURE_FILESHARE_NAME=nivai-data
EXTERNAL_DATA_MOUNT=/mnt/nivai-data
AZURE_RESOURCE_GROUP=nivai-resources

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=nivai
DB_PASSWORD=developmentpassword
DB_NAME=nivai

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Server Configuration
SERVER_PORT=8080